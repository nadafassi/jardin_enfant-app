{% extends 'base.html.twig' %}

{% block body %}


    <div class="col-md-6 grid-margin stretch-card" bis_skin_checked="1">
        <div class="card" bis_skin_checked="1">
            <div class="card-body" bis_skin_checked="1">
                <h4 class="card-title">Ajouter un abonnement</h4>

                <form class="forms-sample" method="post">
                    <div class="form-group" bis_skin_checked="1" hidden>
                        <label for="exampleInputName1">Date</label>
                        {{ form_widget(form.date,{'attr': {'class': 'form-control'}})}}                    </div>
                    <div class="form-group" bis_skin_checked="1">
                        <label for="exampleInputEmail3">Type</label>

                        {{ form_widget(form.type,{'attr': {'class': 'form-control'}})}}
                    </div>




                    <div class="form-group" bis_skin_checked="1" hidden>
                        <label for="exampleInputCity1">Jardin </label>
                        {{ form_widget(form.jardin,{'attr': {'class': 'form-control'}})}}
                    </div>
                    <div class="form-group" bis_skin_checked="1">
                        <label for="exampleInputCity1">Enfant</label>
                        {{ form_widget(form.enfant,{'attr': {'class': 'form-control'}})}}
                    </div>



                    <div>
                        <button type="submit" class="btn btn-success mr-2">Ajouter</button>
                        <ul>
                            <li>

                                <a href="{{ path('abonnement_index') }}">Back to the list</a>
                            </li>
                        </ul>
                    </div>
                    <div class="form-group" bis_skin_checked="1" hidden>
                        <label for="exampleInputCity1">Etat</label>
                        {{ form_widget(form.etat,{'attr': {'class': 'form-control'}})}}
                    </div>
                </form>
            </div>
        </div>
    </div>



{% endblock %}
