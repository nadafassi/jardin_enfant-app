

{% extends 'base.html.twig' %}

{% block body %}


    <section class="contact-form-section">
        <div class="auto-container">
            <div class="contact-form">
                <div class="sec-title text-center">
                    <span class="title">Vous avez une jardin?Rejoignez nous</span>
                    <h2>Entrez Vos Informations</h2>
                </div>
                {{ form_start(form) }}

                <div class="row">
                    <div  id="qr" class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form1.nom,{ 'attr': {'placeholder': 'votre nom'} }) }}
                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form1.username,{ 'attr': {'placeholder': 'username'} }) }}

                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form1.plainPassword.first,{ 'attr': {'placeholder': 'votre mot de passe'} }) }}
                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form1.plainPassword.second,{ 'attr': {'placeholder': 'votre mot de passe'} }) }}
                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form1.email,{ 'attr': {'placeholder': 'email'} }) }}
                    </div>

                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form.name,{ 'attr': {'placeholder': 'nom de jardin'} }) }}
                    </div>

                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form.description,{ 'attr': {'placeholder': 'description de jardin'} }) }}
                    </div>

                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form.adresse,{ 'attr': {'placeholder': 'adresse'} }) }}
                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form.tarif,{ 'attr': {'placeholder': 'tarif'} }) }}
                    </div>
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        {{ form_widget(form.numtel,{ 'attr': {'placeholder': 'num tel'} }) }}
                    </div>



                    <div class="form-group col-lg-12 col-md-12 col-sm-12 text-center">
                        <button class="theme-btn btn-style-one" type="submit" name="submit-form">s'inscrire</button>
                        {{ form_end(form) }}
                    </div>
                </div>
                </form>
            </div>
        </div>
    </section>




    {% endblock %}

