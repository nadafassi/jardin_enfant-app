{% extends 'back.html.twig' %}

{% block nav %}
    <div class="header bg-gradient-primary pb-8 pt-5 pt-md-8">
        <div class="container-fluid">
            <center><h1>Modifier un club</h1></center>
        </div></div>
{% endblock nav %}
{% block body %}
    <center>
<div class="col-md-6 grid-margin stretch-card" bis_skin_checked="1">
    <div class="card" bis_skin_checked="1">
        <div class="card-body" bis_skin_checked="1">

            {{ form_start(edit_form) }}
            <form method="POST" class="forms-sample">
                <div class="form-group" bis_skin_checked="1">
                    <label for="exampleInputName1">Name</label>
                    {{ form_widget(edit_form.name,{'attr': {'class': 'form-control'}})}}                    </div>
                <div class="form-group" bis_skin_checked="1">
                    <label for="exampleInputEmail3">Description</label>

                    {{ form_widget(edit_form.description,{'attr': {'class': 'form-control'}})}}
                </div>
                <div class="form-group" bis_skin_checked="1">
                    <label for="exampleInputCity1">Jardin</label>
                    {{ form_widget(edit_form.jardin,{'attr': {'class': 'form-control'}})}}
                </div>
                <div class="form-group" bis_skin_checked="1">
                    <label for="exampleInputEmail3">photo</label>

                    {{ form_widget(edit_form.photo,{'attr': {'class': 'form-control'}})}}
                </div>
                <div class="form-group col-lg-12 col-md-12 col-sm-12 ">
                    <button type="submit" class="btn btn-success mr-2">Editer</button>
                </div>

                {{ form_end(edit_form) }}
            </form>

        </div>
    </div>
</div>
    <ul>
        <li>
            <a href="{{ path('club_indexs') }}">Back to the list</a>
        </li>

    </ul>




    </center>
{% endblock %}
